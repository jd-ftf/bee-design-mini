{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": 2019,
    "parser": "babel-eslint",
    "sourceType": "module"
  },
  "globals": {
    "Component": true,
    "Page": true,
    "getCurrentPages": true,
    "jd": true,
    "Behavior": true
  },
  "plugins": [
    "json"
  ],
  "extends": [
    "eslint:recommended",
    "standard",
    "plugin:json/recommended"
  ],
  "rules": {
    "new-cap": "off",
    "indent": ["error",2],
    "eol-last":["error","never"],
    "no-prototype-builtins": "off",
    "no-void": "off"
  }
}
